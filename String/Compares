#include <stdio.h>
#include <string.h>

// Compares two strings

void main() {
	
    char str1[] = "apple";
    char str2[] = "banana";
    int result = strcmp(str1, str2);
    
    if (result == 0)
        printf("Strings are equal.\n");
    else if (result < 0)
        printf("String 1 is smaller.\n");
    else
        printf("String 2 is smaller.\n");
        
        
    char str3[6] = {'R', 'O', 'H', 'I'};
    char str4[7] = {'R', 'O', 'H', 'I', 'N', 'I'};
    int result1 = strcmp(str3, str4);
    
    if (result == 0)
        printf("Strings are equal.\n");
    else if (result < 0)
        printf("String 3 is smaller.\n");
    else
        printf("String 4 is smaller.\n");
    
}
